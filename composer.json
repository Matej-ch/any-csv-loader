{
    "name": "matejch/yii2-any-csv-loader",
    "description": "Loading data from any csv to database table",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","csv","loader"],
    "license": "MIT",
    "authors": [
        {
            "name": "Matej Ch",
            "email": "matejCH.work@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.40",
        "league/csv": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "matejch\\anyCsvLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "matejch\\anyCsvLoader\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
